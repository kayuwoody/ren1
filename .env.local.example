# For local development when Codespaces is blocked
# Copy this to .env.local and run locally on your machine

# WooCommerce API
WC_CONSUMER_KEY=your_key_here
WC_CONSUMER_SECRET=your_secret_here
WC_API_URL=https://coffee-oasis.com.my/wp-json/wc/v3

# Fiuu Payment Gateway
FIUU_MERCHANT_ID=your_merchant_id_here
FIUU_VERIFY_KEY=your_verify_key_here
FIUU_SECRET_KEY=your_secret_key_here
FIUU_SANDBOX_MODE=true

# Application URLs (for Fiuu callbacks)
NEXT_PUBLIC_APP_URL=http://localhost:3000
